function promptcolors {
	for color in `fgrep \\e ~/dotfiles/bash/config | fgrep -v '#' | cut -f1 -d=`
	do
		echo -e ${!color}$color$NO_COLOR
	done
}

function ovj {
	ssh kiwiple@$1.ovjet.com
}